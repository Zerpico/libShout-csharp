<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>LibshoutSharp</AssemblyName>
    <RootNamespace>LibshoutSharp</RootNamespace>
    <Platforms>x86;x64</Platforms>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\Libs\x64\**" Link="x64\%(RecursiveDir)%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" />
		<None Include="..\Libs\x86\**" Link="x86\%(RecursiveDir)%(Filename)%(Extension)" CopyToOutputDirectory="PreserveNewest" />		
	</ItemGroup>

</Project>
